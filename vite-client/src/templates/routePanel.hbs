<div hidden id="{{ HtmlElementId.RoutePanel }}">
  <table class="route-table">
    <tr>
      <td><label for="{{ HtmlElementId.StartInput }}">Start:</label></td>
      <td><input type="text" id="{{ Html.StartInput }}" placeholder="Click to select start point"></td>
    </tr>
    <tr>
      <td><label for="{{ Html.EndInput }}">End:</label></td>
      <td><input type="text" id="{{ Html.EndInput }}" placeholder="Click to select end point"></td>
    </tr>
  </table>
</div>
